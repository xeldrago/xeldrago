{
	"$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
	"version": "1.0.0",
	"manifestVersion": "1.16",
	"id": "${{TEAMS_APP_ID}}",
	"name": {
		"short": "Xeldrago - Tooler",
		"full": ""
	},
	"developer": {
		"name": "Teams App, Inc.",
		"mpnId": "",
		"websiteUrl": "https://www.example.com",
		"privacyUrl": "https://www.example.com/termofuse",
		"termsOfUseUrl": "https://www.example.com/privacy"
	},
	"description": {
		"short": "Short description",
		"full": "Full description"
	},
	"icons": {
		"outline": "outline.png",
		"color": "color.png"
	},
	"accentColor": "#FFFFFF",
	"validDomains": [],
	"bots": [
		{
			"botId": "${{BOT_ID}}",
			"scopes": [
				"personal",
				"team",
				"groupchat"
			],
			"supportsFiles": false,
			"isNotificationOnly": false,
			"commandLists": [
				{
					"scopes": [
						"personal",
						"team",
						"groupchat"
					],
					"commands": [
						{
							"title": "Hi",
							"description": "Say hi to the bot."
						}
					]
				}
			]
		}
	],
	"composeExtensions": [],
	"staticTabs": [],
	"configurableTabs": [],
	"permissions": [
		"identity",
		"messageTeamMembers"
	]
}